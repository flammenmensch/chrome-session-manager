{
  "manifest_version": 2,
  "name": "__MSG_appName__",
  "version": "1.0.0",
  "version_name": "Alpha",
  "default_locale": "ru",
  "browser_action": {
    "default_title": "Save current session",
    "default_popup": "page-action.html"
  },
  "author": "Alexey Protasov",
  "background": {
    "persistent": false,
    "scripts": [ "background.bundle.js" ]
  },
  "minimum_chrome_version": "49",
  "incognito": "split",
  "permissions": [
    "sessions",
    "storage",
    "tabs"
  ]
}
